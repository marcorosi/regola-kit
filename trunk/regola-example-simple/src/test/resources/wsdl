          <s:element minOccurs="0" maxOccurs="1" name="UserId" type="s:string"/>
          <s:element minOccurs="0" maxOccurs="1" name="UniboId" type="s:string"/>
        </s:sequence>
      </s:complexType>
      <s:simpleType name="AuthenticationStateType">
        <s:restriction base="s:string">
          <s:enumeration value="Ok"/>
          <s:enumeration value="BadAuthentication"/>
          <s:enumeration value="NotDefined"/>
        </s:restriction>
      </s:simpleType>
      <s:simpleType name="UserStateType">
